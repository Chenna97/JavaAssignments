package com.abs.quest1;

public class Developer extends TeamLead {
	
	 String hobbies[];

	 public Developer(String empName, double salary, int empId, String[] hobbies) {
		super(empName, salary, empId);
		this.hobbies = hobbies;
	 }

	 @Override
	 void showProjects() {
		 System.out.println("Projects in internal project");
		
	 } 
	 
	 @Override
		void printDetails()
		{
			//print all the instance variables
			System.out.println("Employee Name is ::" + empName);
			System.out.println("Salary of Employee is :: " + salary);
			System.out.println("Employee ID is :: "+ empId);
			System.out.println("Company Name is :: "+ COMPANYNAME);
		
		} 

	 
	 void showHobbies(){ 
		 syso(hobbies+"is hobby"); 
	 }
}
