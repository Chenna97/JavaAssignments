package com.basics.quest2;

public class Bank {
	
	double balance;
	
	
	
	
	 public Bank(double balance) {
		super();
		this.balance = balance;
	}

	 void withdraw(double amount) {
		 
		 if((balance > 0) && (balance-amount !=0) ){
			 System.out.println("you can withdraw money of : ");
		 }
		 
		 else{
			 System.out.println(" your account dont have enough money to withdraw"); 
			 
		 }
		 
	 }
	 
	 void deposit(double amount) {
		 
		 System.out.println("Deposited successfully of "+amount + " into you account");
		 
	 }
	 
	 double getBalance(){
		 
		 return balance;
	 }
}
